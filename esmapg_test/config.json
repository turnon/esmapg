{
    "orders": {
        "only": ["order_no", "created_at"],
        "has_many": {
            "line_items": {
                "only": ["barcode"],
                "has_one": {
                    "goods": {
                        "only": ["code"]
                    }
                },
                "has_many": {
                    "discounts": {
                        "only": ["amount"],
                        "has_one": {
                            "promotions": {
                                "only": ["code"]
                            }
                        }
                    }
                }
            }

        },
        "has_one": {
            "contract": {
                "only": ["text"]
            }
        },
        "belongs_to": {
            "members": {
                "only": ["name", "mobile"],
                "has_many": {
                    "addresses": {
                        "only": ["path"]
                    }
                }
            },
            "stores": {
                "only": ["name"]
            }
        }
    }
}